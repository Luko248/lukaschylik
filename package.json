{
  "name": "lukas-chylik-portfolio",
  "description": "My personal portfolio website using Qwik, Vite, pnpm, styleX",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "clean": "rm -rf dist server node_modules/.vite",
    "prebuild": "npm run clean",
    "build": "qwik build",
    "build.client": "vite buiSld",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "predev": "npm run clean",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-syntax-flow": "^7.24.1",
    "@babel/plugin-syntax-jsx": "^7.24.1",
    "@babel/plugin-syntax-typescript": "^7.24.1",
    "@builder.io/qwik": "^1.5.2",
    "@builder.io/qwik-city": "^1.5.2",
    "@stylexjs/babel-plugin": "^0.5.1",
    "@stylexjs/eslint-plugin": "^0.5.1",
    "@types/eslint": "^8.56.8",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-plugin-qwik": "^1.5.2",
    "postcss": "^8.4.38",
    "postcss-preset-env": "^9.5.5",
    "prettier": "^3.2.5",
    "sass": "^1.74.1",
    "typescript": "^5.4.5",
    "undici": "^6.12.0",
    "vite": "^5.2.8",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-stylex": "^0.8.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@stylexjs/stylex": "^0.5.1"
  }
}