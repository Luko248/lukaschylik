{
  "name": "lukas-chylik-portfolio",
  "description": "My personal portfolio website using Qwik, Vite, pnpm, styleX",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "clean": "rm -rf dist server node_modules/.vite", 
    "prebuild": "npm run clean",
    "build": "qwik build",
    "build.client": "vite buiSld",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "predev": "npm run clean",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-flow": "^7.26.0",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/plugin-syntax-typescript": "^7.25.9",
    "@builder.io/qwik": "^1.9.1",
    "@builder.io/qwik-city": "^1.9.1",
    "@stylexjs/babel-plugin": "^0.9.3",
    "@stylexjs/eslint-plugin": "^0.9.3",
    "@types/eslint": "^8.56.12",
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-qwik": "^1.9.1",
    "postcss": "^8.4.48",
    "postcss-preset-env": "10.0.9",
    "prettier": "^3.3.3",
    "sass": "^1.80.6",
    "typescript": "^5.6.3",
    "undici": "^6.20.1",
    "vite": "^5.4.10",
    "vite-plugin-inspect": "^0.8.7",
    "vite-plugin-stylex": "^0.8.3",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@stylexjs/stylex": "^0.9.3"
  }
}