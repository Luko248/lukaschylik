{
  "name": "my-portfolio-website",
  "description": "",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build:css": "sass src/styles/scss/index.scss src/styles/css/index.css",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "npm run format && concurrently \"vite --open --mode ssr\" \"npm run watch:css\"",
    "watch:css": "nodemon",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.12.0",
    "@builder.io/qwik-city": "^1.12.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.12.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "eslint-plugin-qwik": "^1.12.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "tailwindcss": "^4.0.0",
    "tailwindcss-signals": "^0.0.25",
    "typescript": "^5.7.3",
    "undici": "^7.3.0",
    "vite": "^6.0.11",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "tailwind-variants": "^1.0.0"
  }
}
